<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">
    <record id="index_config_product_product" model="se.index.config">
        <field name="name">product_product_config</field>
        <field name="body">{}</field>
        <field name="body_str">
{
  "settings":{
    "analysis":{
      "char_filter":{
        "replace":{
          "type":"mapping",
          "mappings":[
            "&amp;=> and "
          ]
        }
      },
      "filter":{
        "word_delimiter":{
          "type":"word_delimiter",
          "split_on_numerics":false,
          "split_on_case_change":true,
          "generate_word_parts":true,
          "generate_number_parts":true,
          "catenate_all":true,
          "preserve_original":true,
          "catenate_numbers":true
        }
      },
      "analyzer":{
        "default":{
          "type":"custom",
          "char_filter":[
            "html_strip",
            "replace"
          ],
          "tokenizer":"whitespace",
          "filter":[
            "lowercase",
            "word_delimiter"
          ]
        }
      },
      "index" : {
        "sort.field" : ["priority", "main", "id"],
        "sort.order" : ["asc", "desc", "asc"]
      }
    }
  },
  "mappings":{
    "properties":{
      "hierarchicalCategories":{
        "type":"nested",
        "properties":{
          "ancestors":{
            "type":"keyword"
          },
          "order":{
            "type":"integer"
          },
          "value":{
            "type":"keyword"
          },
          "level":{
            "type":"integer"
          }
        }
      },
      "url_key":{
        "type":"keyword"
      },
      "variant_attributes":{
        "properties":{
          "color":{
            "type":"text",
            "fielddata":true
          }
        }
      },
      "main":{
        "type":"boolean"
      },
      "id":{
          "type":"integer"
      },
      "priority":{
          "type":"integer"
      },
      "redirect_url_key":{
        "type":"keyword"
      }
    }
  }
}
        </field>
    </record>

