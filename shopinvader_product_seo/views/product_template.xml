<odoo>

    <record model="ir.ui.view" id="product_template_form_view">
        <field name="name">product.template.form (in shopinvader_product_seo)</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="shopinvader_product.product_template_form_view" />
        <field name="arch" type="xml">
            <page name="shopinvader" position="inside">
                <group name="meta">
                    <field name="seo_title" />
                    <field name="meta_keywords" />
                    <field name="meta_description" />
                </group>
            </page>
        </field>
    </record>

</odoo>
